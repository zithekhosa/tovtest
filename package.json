{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "dev:client": "vite",
    "dev:server": "tsx server/index.ts",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:optimized": "node scripts/start-optimized.js",
    "build": "vite build --outDir client/dist",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:vercel": "npm run build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/public",
    "preview:prod": "npm run build:prod && npm run start",
    "start": "cross-env NODE_ENV=production tsx server/index.ts",
    "check": "tsc",
    "test:performance": "vitest --run tests/performance",
    "test:performance:continuous": "tsx scripts/performance-test.ts --continuous",
    "performance:test": "cross-env NODE_ENV=production npm run build && npm run test:performance",
    "optimize": "tsx scripts/apply-production-optimizations.ts",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test:setup": "node test-setup.js",
    "debug": "node scripts/debug-runtime-errors.cjs",
    "check:errors": "npm run debug && npm run build",
    "health-check": "tsx scripts/health-check.ts",
    "health-check:watch": "tsx scripts/health-check.ts --watch",
    "validate-apis": "tsx scripts/validate-apis.ts",
    "security-audit": "tsx scripts/security-audit.ts",
    "validate-env": "tsx scripts/validate-environment.ts",
    "test:infrastructure": "npm run validate-env && npm run health-check && npm run validate-apis",
    "phase1:complete": "npm run test:infrastructure && npm run security-audit",
    "test:error-handling": "tsx scripts/test-error-handling.ts",
    "test:supabase": "tsx scripts/test-supabase-connection.ts",
    "test:simple-health": "tsx scripts/simple-health-test.ts",
    "test:error-manager": "tsx scripts/test-error-manager.ts",
    "test:error-manager:simple": "node test-error-manager.js",
    "test:reliability": "tsx scripts/test-reliability-system.ts",
    "test:reliability:full": "npm run health-check && npm run test:error-handling && npm run test:error-manager && npm run test:reliability",
    "monitor:performance": "tsx scripts/performance-monitor.ts",
    "system:optimize": "tsx scripts/system-optimizer.ts",
    "schema:apply-maintenance-workflow": "tsx scripts/apply-maintenance-workflow-schema.ts",
    "schema:test-maintenance-workflow": "tsx scripts/test-maintenance-workflow-schema.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run tests/unit",
    "test:integration": "vitest --run tests/integration",
    "test:e2e": "vitest --run tests/e2e",
    "test:workflow": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "seed:mock-data": "tsx scripts/run-seed-data.ts",
    "test:database": "tsx scripts/test-database-migration.ts",
    "migrate:to-database": "npm run db:push && npm run test:database",
    "health-check:system": "tsx scripts/verify-system-health.ts"
  },
